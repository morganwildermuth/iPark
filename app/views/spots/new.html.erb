<%= form_for @spot do |f| %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <%= f.label :street %>
  <%= f.text_field :street %>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :location_type %>
  <%= f.text_field :location_type %>
  <%= f.submit %>
<% end %>

